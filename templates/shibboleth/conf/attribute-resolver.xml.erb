                    nameFormat="urn:oasis:names:tc:SAML:2.0:nameid-format:transient" />
            <ad:Script><![CDATA[
                    
                    campus_fyindicator.getValues().clear(); // remove the dummy value


                    if(studentType.getValues().size() > 0) {
                            if (studentType.getValues().get(0) == "firsttime") {
                                    fyindicator = "firstyear";
                            } else {
                                    fyindicator = "continuing";
                            }
                    } else {
                            fyindicator = "nonstudent";
                    }
                    
                    if(muohioeduPrimaryLocation.getValues().size() > 0) {
                            campus_fyindicator.getValues().add(muohioeduPrimaryLocation.getValues().get(0) + "_" + fyindicator);
                    } else {
                            campus_fyindicator.getValues().add("unknownLocation_" + fyindicator);
                    }
                    ]]></ad:Script>
    </resolver:AttributeDefinition>

