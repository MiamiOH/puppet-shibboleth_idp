    <resolver:AttributeDefinition xsi:type="ad:Script" id="sciquestRole" >
        <resolver:Dependency ref="muAD" />
        <resolver:Dependency ref="isMemberOf" />
        <resolver:Dependency ref="forSciquestRoleScript"/>
        <resolver:AttributeEncoder xsi:type="enc:SAML1String" 
            name="urn:mace:dir:attribute-def:sciquestRole" />
        <resolver:AttributeEncoder xsi:type="enc:SAML2String" 
            name="http://miamioh.edu/idp/attributes/sciquestRole" friendlyName="sciquestRole" />
        <resolver:AttributeEncoder xsi:type="enc:SAML2StringNameID" 
            nameFormat="urn:oasis:names:tc:SAML:2.0:nameid-format:transient" />

        <ad:Script><![CDATA[

            sciquestRole.getValues().clear(); // remove the dummy value

            // Add at least one value
            counter=0;

            sciquestRole.getValues().add("Tester");

            // If the user has group membership
            if (typeof isMemberOf != "undefined" && isMemberOf != null ){
                // The go through each group membership and add the appropriate affiliation
                // The IdP will remove duplicate values so we don't need to worry about that here
                for ( i = 0; isMemberOf != null && i < isMemberOf.getValues().size(); i++ ) {
                    value = isMemberOf.getValues().get(i);
                    if (value.indexOf("acp-buyer")>0) {
                        sciquestRole.getValues().add("Buyer");
                        counter++;
                    }
                    if (value.indexOf("acp-requestor")>0) {
                        sciquestRole.getValues().add("Requestor");
                        counter++;
                    }
                    if (value.indexOf("acp-approver")>0) {
                        sciquestRole.getValues().add("Approver");
                        counter++;
                    }
                    if (value.indexOf("dfbs-apclerk")>0) {
                        sciquestRole.getValues().add("AP Clerk");
                        counter++;
                    }
                    if (value.indexOf("dfbs-apmanager")>0) {
                        sciquestRole.getValues().add("AP Manager");
                        counter++;
                    }
                    if (value.indexOf("dfbs-catadmin")>0) {
                        sciquestRole.getValues().add("Catalog Administrator");
                        counter++;
                    }
                    if (value.indexOf("dfbs-contractadmin")>0) {
                        sciquestRole.getValues().add("Contract Administrator");
                        counter++;
                    }
                    if (value.indexOf("dfbs-helpdesk")>0) {
                        sciquestRole.getValues().add("Help Desk Support");
                        counter++;
                    }
                    if (value.indexOf("dfbs-dept_nonpo_inv")>0) {
                        sciquestRole.getValues().add("NonPO Invoice");
                        counter++;
                    }

                }
            }
        ]]></ad:Script>

    </resolver:AttributeDefinition>

    <resolver:DataConnector xmlns="urn:mace:shibboleth:2.0:resolver:dc" xsi:type="dc:Static" id="forSciquestRoleScript">
        <Attribute id="sciquestRole">
            <Value>dummy</Value>
        </Attribute>
    </resolver:DataConnector>



