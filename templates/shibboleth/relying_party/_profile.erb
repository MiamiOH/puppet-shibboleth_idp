        <bean parent="RelyingPartyByName" c:relyingPartyIds="<%= @entity_id %>">
            <%- @properties.each do |property| -%>
            <property name="<%= property['name'] %>">
                <list>
                    <%- property['beans'].each do |bean| -%>
                    <bean parent="<%= bean['parent'] %>"
                      <%= bean['attributes'].map{ |k,v| %Q(p:#{k}="#{v}") }.join(' ')%> />
                    <%- end -%>
                </list>
            </property>
            <%- end -%>
        </bean>
